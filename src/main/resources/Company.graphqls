type Company {
	id: BigInteger!
	cnpj: String!
	name: String!
	kpis: [Kpi]!
	cnpjs: [String]!
}

input CompanyFilter {
	id: BigInteger
	cnpj: String
	name: String
}

extends type Query {
	findCompany(filter: CompanyFilter): [Company]
	findCompany(email: String): [Company]
	
}

extends type Mutation {

}
    
schema {
	query: Query
	mutation: Mutation
}
